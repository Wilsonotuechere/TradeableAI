{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "noEmit": false,
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true
  },
  "include": ["server/**/*"],
  "exclude": ["node_modules", "client/**/*"]
}
